        <dc:title>Deep Learning With Python</dc:title>
        <dc:creator opf:file-as="Chollet, Francois" opf:role="aut">Francois Chollet</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (3.39.1) [https://calibre-ebook.com]</dc:contributor>
